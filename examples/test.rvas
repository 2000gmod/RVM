global str1 {
    $"Hello!\n"
}

function factorial {
    load [0]
    loadconst !i64 0
    eq @i64
    jmpif BASE
    load [0]
    load [0]
    loadconst !i64 1
    sub @i64
    call [1] $"factorial"
    mul @i64
    ret [1]
label BASE
    loadconst !i64 1
    ret [1]
}

function fibo {
    load [0]
    loadconst !i64 0
    eq @i64
    load [0]
    loadconst !i64 1
    eq @i64
    lor
    jmpif L1
    load [0]
    loadconst !i64 1
    sub @i64
    call [1] $"fibo"
    load [0]
    loadconst !i64 2
    sub @i64
    call [1] $"fibo"
    add @i64
    ret [1]
label L1
    loadconst !i64 1
    ret [1]
}

function main {
    getglobal $"str1"
    call [1] $"__printstr"

    loadconst !i64 20
    call [1] $"fibo"
    call [1] $"__printi64"
    call [0] $"__printnl"

    loadconst !i64 20
    call [1] $"factorial"
    call [1] $"__printi64"
    call [0] $"__printnl"
    ret [0]
}